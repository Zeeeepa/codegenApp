# CodegenApp CI/CD Flow Management System Environment Configuration
# Copy this file to .env and fill in your actual values

# ===== CORE CODEGEN API CONFIGURATION =====
# REQUIRED: Get your API token from https://app.codegen.com/settings
REACT_APP_API_TOKEN=your_api_token_here
REACT_APP_DEFAULT_ORGANIZATION=your_org_id_here
CODEGEN_API_TOKEN=your_api_token_here
CODEGEN_ORG_ID=your_org_id_here

# API Base URL - uses proxy server to avoid CORS issues
# Development: http://localhost:8001/api (uses local proxy server)
# Production: https://api.codegen.com (direct API access)
REACT_APP_API_BASE_URL=http://localhost:8001/api
CODEGEN_API_BASE=https://api.codegen.com

# User Configuration
# REQUIRED: Your user ID for authentication
REACT_APP_USER_ID=your_user_id_here

# DEVELOPMENT MODE: Set to 'true' to enable demo mode without real API token
REACT_APP_DEMO_MODE=false

# ===== BACKEND CONFIGURATION =====
FRONTEND_URL=http://localhost:8000
BACKEND_PORT=8001
BACKEND_HOST=0.0.0.0

# ===== GITHUB API CONFIGURATION =====
# REQUIRED: GitHub personal access token for API access
# Get your token from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here
GITHUB_API_BASE=https://api.github.com

# ===== AI/ML API CONFIGURATION =====
# REQUIRED: Gemini API key for AI-powered features
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-pro-latest
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_TOKENS=8192

# ===== CLOUDFLARE WORKERS CONFIGURATION =====
# REQUIRED: Get your API key from https://dash.cloudflare.com/profile/api-tokens
CLOUDFLARE_API_KEY=your_cloudflare_api_key_here
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id_here
CLOUDFLARE_WORKER_NAME=webhook-gateway
CLOUDFLARE_WORKER_URL=https://webhook-gateway.your-subdomain.workers.dev

# ===== WEBHOOK CONFIGURATION =====
WEBHOOK_BASE_URL=https://your-codegenapp-domain.com
WEBHOOK_TIMEOUT=30000
WEBHOOK_MAX_RETRIES=3
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here
WEBHOOK_DEBUG_MODE=false

# ===== WEB-EVAL-AGENT CONFIGURATION =====
WEB_EVAL_MCP_PATH=web-eval-agent
WEB_EVAL_TIMEOUT=300000
WEB_EVAL_MAX_CONCURRENT=3
WEB_EVAL_BROWSER=chromium
WEB_EVAL_HEADLESS=true

# ===== GRAPH-SITTER CONFIGURATION =====
GRAPH_SITTER_CACHE_SIZE=1000
GRAPH_SITTER_MAX_FILE_SIZE=1048576
GRAPH_SITTER_SUPPORTED_LANGUAGES=python,javascript,typescript,go,rust,java,cpp,c
GRAPH_SITTER_TIMEOUT=60000

# ===== VALIDATION PIPELINE CONFIGURATION =====
VALIDATION_TIMEOUT=1800
VALIDATION_MAX_RETRIES=3
VALIDATION_CONFIDENCE_THRESHOLD=0.8
VALIDATION_SNAPSHOT_TTL=3600
VALIDATION_CONCURRENT_LIMIT=5

# ===== DEPLOYMENT CONFIGURATION =====
DEPLOYMENT_TIMEOUT=600
DEPLOYMENT_MAX_RETRIES=2
DEPLOYMENT_LOG_LEVEL=INFO
DEPLOYMENT_CLEANUP_TIMEOUT=300

# ===== AUTO-MERGE CONFIGURATION =====
AUTO_MERGE_ENABLED=true
AUTO_MERGE_CONFIDENCE_THRESHOLD=0.9
AUTO_MERGE_ERROR_THRESHOLD=0
AUTO_MERGE_REQUIRE_TESTS=true

# ===== SANDBOX CONFIGURATION =====
SANDBOX_PROVIDER=modal
SANDBOX_TIMEOUT=3600
SANDBOX_MAX_MEMORY=2048
SANDBOX_MAX_CPU=2
SANDBOX_CLEANUP_DELAY=300

# ===== SWE-BENCH INTEGRATION =====
SWEBENCH_DATASET=lite
SWEBENCH_MAX_WORKERS=4
SWEBENCH_TIMEOUT=1800
SWEBENCH_CACHE_LEVEL=env

# ===== LOGGING CONFIGURATION =====
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE_PATH=logs/codegenapp.log
LOG_MAX_SIZE=100MB
LOG_BACKUP_COUNT=5

# ===== REDIS CONFIGURATION (Optional) =====
REDIS_URL=redis://localhost:6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_TIMEOUT=5

# ===== DATABASE CONFIGURATION (Optional) =====
DATABASE_URL=sqlite:///./codegenapp.db
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# ===== MONITORING CONFIGURATION =====
METRICS_ENABLED=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30
PERFORMANCE_MONITORING=true

