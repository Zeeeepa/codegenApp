{
  "name": "github.gg",
  "version": "1.0.0",
  "description": "GitHub repository analyzer and webhook processor",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "install:legacy": "npm install --legacy-peer-deps",
    "setup": "npm run install:legacy && npm run build",
    "webhook:test": "node scripts/test-webhook.js"
  },
  "dependencies": {
    "@octokit/app": "^14.0.2",
    "@octokit/auth-app": "^6.0.1",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks-types": "^7.3.2",
    "next": "^14.0.0",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.0"
  },
  "keywords": [
    "github",
    "webhook",
    "repository",
    "analyzer",
    "nextjs",
    "typescript"
  ],
  "author": "Zeeeepa",
